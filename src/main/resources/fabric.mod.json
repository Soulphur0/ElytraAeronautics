{
  "schemaVersion": 1,
  "id": "elytra_aeronautics",
  "version": "${version}",

  "name": "Elytra aeronautics",
  "description": "Fly faster with the elytra the higher you travel and customise cloud layers at will! \nThis mod is highly configurable, use the '/ean' command without extra arguments for more information.",
  "authors": [
    "Soulphur"
  ],
  "contact": {
    "homepage": "https://github.com/Soulphur0/ElytraAeronautics",
    "sources": "https://github.com/Soulphur0/ElytraAeronautics",
    "issues": "https://github.com/Soulphur0/ElytraAeronautics/issues"
  },

  "license": "CC-BY-NC-SA-3.0",
  "icon": "assets/elytra_aeronautics/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.Soulphur0.ElytraAeronautics"
    ],
    "client": [
      "com.github.Soulphur0.ElytraAeronauticsClient"
    ],
    "preLaunch": [
      "com.llamalad7.mixinextras.MixinExtrasBootstrap::init"
    ]
  },
  "mixins": [
    "elytraaeronautics.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.15.6",
    "fabric": ">=0.95.1+1.20.4",
    "minecraft": "1.20.4",
    "java": ">=17"
  },
  "suggests": {
    "modmenu": "*"
  }
}
